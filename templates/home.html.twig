{% extends 'base.html.twig' %}

{% block body %}
<h1>Bienvenue sur ma homepage</h1>

<h3>Table de mes technos</h3>
<div class="row">

    {% for techno in technos %}
      <div class="card" style="width: 18rem;">
        <img src="{{ techno.image }}" class="card-img-top" alt="...">
        <div class="card-body">
          <p class="card-text">{{ techno.name }}</p>
        </div>
      </div>


    {% endfor %}
    
</div>


{% endblock %}


